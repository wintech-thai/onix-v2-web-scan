{
  "name": "onix-v2-web-scan",
  "version": "2.0.0",
  "description": "Onix v2 Web Scan - Verification system for encrypted QR code and scan data",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node -e \"require('dotenv').config(); const port = process.env.PORT || 5001; require('child_process').spawn('next', ['dev', '-p', port], {stdio: 'inherit'})\"",
    "build": "next build",
    "start": "node -e \"require('dotenv').config(); const port = process.env.PORT || 5001; require('child_process').spawn('next', ['start', '-p', port], {stdio: 'inherit'})\"",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "@types/canvas-confetti": "^1.9.0",
    "canvas-confetti": "^1.9.3",
    "dotenv": "^17.2.3",
    "ioredis": "^5.4.1",
    "lucide-react": "^0.545.0",
    "next": "^15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.16.0",
    "eslint-config-next": "^15.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  }
}
